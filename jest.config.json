{
    "roots": ["./tests"],
    "transform": {
        "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": ["ts", "js", "json", "node"],
    "collectCoverage": false,
    "coverageReporters": ["json", "lcov", "text", "clover"],
    "coverageDirectory": "coverage",
    "testEnvironment": "node"
}
